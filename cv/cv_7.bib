keywords = {13-07-12127,14-07-31194,mypubtr,scopus},
mendeley-groups = {my{\_}publications},
mendeley-tags = {13-07-12127,14-07-31194,mypubtr,scopus},
number = {6},
pages = {420--425},
title = {{A Technique for Retrieving Cause and Effect Relationships from Optimized Fact Bases}},
url = {http://link.springer.com/article/10.3103/S0147688215060039},
volume = {42},
year = {2015}
}
