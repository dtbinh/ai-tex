isbn = {978-1-5090-1353-1},
keywords = {13-07-12127,15-59-31516,anticancer vaccine,aq-method,causal relations,data mining,dendritic cell therapy,genetic algorithm,jsm-method,myconf,natural language processing},
mendeley-groups = {my{\_}publications},
