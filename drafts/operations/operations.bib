@article{Panov2014d,
	abstract = {В статье рассматривается задача моделирования зрительного восприятия, приводятся основные принципы построения нисходящих моделей. На основе этих принципов приводится формальное определение распознающего блока, как базового математического объекта большинства моделей восприятия. Описывается алгоритм его работы, на основе которого строятся операторы распознавания. Проводится постановка классической статической и динамической задач распознавания и исследуются свойства корректности построенных операторов с использованием алгебраического подхода Ю.И. Журавлева.},
	author = {Панов, А. И.},
	file = {:C$\backslash$:/Users/sanek/Documents/Mendeley Desktop/Машинное обучение и анализ данных/2014/Панов - 2014.pdf:pdf;:C$\backslash$:/Users/sanek/Documents/Mendeley Desktop/Машинное обучение и анализ данных/2014/Панов - 2014(2).pdf:pdf},
	journal = {Машинное обучение и анализ данных},
	keywords = {14-07-31194,elibrary,mypub,иерархическое динамическое распознавание,компьютерное когнитивное моделирование,корректность операторов распознавания,распознавание образов},
	language = {russian},
	mendeley-tags = {14-07-31194,elibrary,mypub},
	number = {7},
	pages = {863--874},
	title = {{Алгебраические свойства операторов распознавания в моделях зрительного восприятия}},
	volume = {1},
	year = {2014}
}
@incollection{Skrynnik2016,
