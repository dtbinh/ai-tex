isbn = {9781450328838},
issn = {21672148},
journal = {arXiv},
pages = {1--9},
title = {{Efficient Model Learning for Human-Robot Collaborative Tasks}},
url = {http://arxiv.org/abs/1405.6341},
year = {2014}
}
