keywords = {14-07-31194,15-37-20893,behavior planning,coalition,elibrary,knowledge representation,lian,myconf,path planning,scopus,semiotic model,sign world model,task planning,wos},
mendeley-tags = {14-07-31194,15-37-20893,elibrary,myconf,scopus,wos},
