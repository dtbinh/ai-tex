number = {1},
pages = {124--145},
pmid = {21475691},
title = {{Dynamic, Task-Related and Demand-Driven Scene Representation}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3059823{\&}tool=pmcentrez{\&}rendertype=abstract},
