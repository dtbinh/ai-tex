journal = {Neuron},
keywords = {Attention,Attention: physiology,Brain,Brain: physiology,Cognition,Cognition: physiology,Environment,Humans,Nerve Net,Nerve Net: physiology,Orientation,Orientation: physiology},
number = {3},
pages = {306--24},
