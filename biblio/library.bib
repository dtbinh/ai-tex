file = {:C$\backslash$:/Users/Aleksandr/Documents/Mendeley Desktop/Della Penna, Magazzeni, Mercorio/Applied Intelligence/Della Penna, Magazzeni, Mercorio\_2012\_A universal planning system for hybrid domains.pdf:pdf},
