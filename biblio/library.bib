doi = {10.1016/j.neuron.2008.04.017},
file = {:C$\backslash$:/Users/Aleksandr/Documents/Mendeley Desktop/Corbetta, Patel, Shulman/Neuron/Corbetta, Patel, Shulman\_2008\_The reorienting system of the human brain from environment to theory of mind.pdf:pdf},
