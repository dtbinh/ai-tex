keywords = {Brain,Brain: physiology,Consciousness,Consciousness: physiology,Humans,Models,Neural Networks (Computer),Psychological},
number = {9},
pages = {947--54},
pmid = {17890054},
title = {{Hierarchical approaches to understanding consciousness.}},
